<Window x:Class="MainAPP.WindowTweak"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:libActiveX="clr-namespace:Lib_ActiveX;assembly=Lib_ActiveX"
        Title="WindowTweak" Height="300" Width="300" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <WindowsFormsHost Name="Host">
            <WindowsFormsHost.Child>
                <libActiveX:BlockPanel></libActiveX:BlockPanel>
            </WindowsFormsHost.Child>
        </WindowsFormsHost>
    </Grid>
</Window>
